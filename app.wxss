.item1 {
  height: 50rpx;
  width: 100%;
  text-align: center;
  /* background-color:cyan; */
  border: 1px solid #fff;
  /* flex-shrink: 1 */
}

.product_image {
  opacity: 0;
  width: 100%;
  height: 70%;
  transition: opacity1slinear2s;
}

.loaded {
  opacity: 1;
}

.input {
  padding-left: 10px;
  height: 44px;
}

.inputView {
  border: 2px solid blue;
  background: #fff;
  width: 80%;
  margin: 30rpx auto;
  position: relative;
}

.tab-h {
  height: 80rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
  background: #f7f7f7;
  white-space: nowrap;
  position: fixed;
  top: 0;
  left: 0;
  z-index: 99;
}

.tab-orders {
  height: 80rpx;
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  line-height: 80rpx;
  background: #f7f7f7;
  white-space: nowrap;
  position: fixed;
  /* top: 0; *//* left: 0; */
  z-index: 99;
}

.tab-item {
  margin: 0 25rpx;
  display: inline-block;
}

.tab-item.active {
  color: #4675f9;
  position: relative;
}

.tab-item.active:after {
  content: "";
  display: block;
  height: 8rpx;
  /* width: 48rpx; */
  background: #4675f9;
  position: absolute;
  bottom: 0;
  left: 5rpx;
  border-radius: 16rpx;
}
